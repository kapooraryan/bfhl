{
  "version": 2,
  "builds": [
    {
      "src": "bfhl-backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "bfhl-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/bfhl",
      "dest": "/bfhl-backend/index.js"
    }
  ]
}
